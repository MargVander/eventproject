<h1 class="font-weight-light" align="center">Where are we going ?</h1><br>

<div class="list-group">

<% @validatedevent.each do |event| %>

<a href="<%=event_path(event.id)%>" class="list-group-item list-group-item-action">
    <div class="d-flex w-100 justify-content-between">
      <h5 class="mb-1 text-dark font-weight-bold"><%= event.title %></h5>
    </div>
    <p class="mb-1 text-muted"><%= event.admin.email %></p>
  </a>

  <% end %>
</div>
